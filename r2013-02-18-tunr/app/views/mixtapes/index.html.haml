%h3= "Mixtapes for #{@auth.name}"
%table
  - @mixtapes.each do |mixtape|
    %tr
      %td= link_to mixtape.name, mixtape
      %td= link_to "Edit", edit_mixtape_path(mixtape.id), :class=>'button radius tiny'
      %td= link_to "Delete", mixtape, :method=>'delete', :confirm=>'Are you sure?', :class=>'button radius tiny'
